---
import FeatureCard from "~/components/feature-card.astro";

const { data } = Astro.props;
const { contentList } = data;
const { ContentIDs } = contentList;
---

<ul class="grid max-w-6xl grid-cols-1 md:grid-cols-2 lg:grid-cols-3">
  {
    contentList &&
      ContentIDs &&
      ContentIDs.length > 0 &&
      data.contentList.ContentIDs.map((id: string) => (
        <FeatureCard contentId={id} />
      ))
  }
</ul>
